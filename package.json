{
	"name": "korail",
	"version": "1.0.1",
	"description": "Client for the South Korean korail railway API.",
	"keywords": [
		"asia",
		"fptf",
		"korail",
		"korea",
		"rail",
		"railway",
		"south",
		"south korea",
		"train"
	],
	"homepage": "https://github.com/juliuste/korail",
	"bugs": "https://github.com/juliuste/korail/issues",
	"repository": "juliuste/korail",
	"license": "ISC",
	"author": "Julius Tens <mail@juliustens.eu>",
	"files": [
		"lib/*",
		"dist/*"
	],
	"main": "./dist/index.js",
	"module": "./lib/index.js",
	"scripts": {
		"check-deps": "depcheck --ignores='babel-*'",
		"fix": "eslint --fix lib test.js",
		"lint": "eslint lib test.js",
		"prepare": "npm run transpile",
		"prepublishOnly": "npm test",
		"test": "npm run lint && npm run check-deps && npm run transpile && node test",
		"transpile": "babel lib --presets babel-preset-es2015 --out-dir dist"
	},
	"dependencies": {
		"fetch-ponyfill": "^6.0.2",
		"lodash": "^4.17.11",
		"luxon": "^1.4.5",
		"query-string": "^6.2.0",
		"slugg": "^1.2.1"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"depcheck": "^0.8.0",
		"eslint": "^5.16.0",
		"eslint-config-standard": "^12.0.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-node": "^9.0.1",
		"eslint-plugin-promise": "^4.1.1",
		"eslint-plugin-standard": "^4.0.0",
		"tape": "^4.10.1",
		"tape-promise": "^4.0.0",
		"validate-fptf": "^3.0.0"
	},
	"engines": {
		"node": ">=8"
	}
